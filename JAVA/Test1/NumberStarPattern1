import java.util.Scanner;
public class runner {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        
        int i=1;
        while(i <= n){
            int j = 1;
            int x = n;
            while(j <= n){
                if(x == i){
                    System.out.print("*");
                    x--;
                }else{
                    System.out.print(x);
                    x--;
                }
                j++;
            }
            System.out.println();
            i++;
        }

	}
}
