import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        // Write your code here
        Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        if(n == 1){
             System.out.println(1);
        }

        for(int i=0;i<=n/2;i++){
            if(i * i > n){
                System.out.println(i-1);
                return;
            }

        }

    }
}
