import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
        
        int x = s.nextInt();
        int i = 1;
        int count = 0;

        while(count != x){
            int y = 3*i + 2;

            if(y % 4 != 0){
                System.out.print(y+" ");
                    count++;
            }
            i++;

        }

    }
}
