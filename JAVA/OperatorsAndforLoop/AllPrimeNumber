import java.util.Scanner;

public class Solution {

    public static void main(String[] args) {

        Scanner s = new Scanner(System.in);
        int n = s.nextInt();

        for(int i=2;i<=n;i++){
            int j = 2;
            for(j=2;j<i;j++){
                if(i%j == 0){
                    break;
                }
            }
            if(j==i){
                System.out.println(i);
            }
        }
    }
}
