public class PushZerosAtEnd {

    public static void pushZerosAtEnd(int[] arr){
        
        int n = arr.length;
        int i=0;
        int k = 0;
        for(i=0;i<n;i++){
            if(arr[i] != 0){
                int temp = arr[i];
                arr[i] = arr[k];
                arr[k] = temp;
                k++;
            }
        }

    }
}
