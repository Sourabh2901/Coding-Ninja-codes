public class solution {
    public static void print2DArray(int input[][]) {

        int n = input.length;
        int m = input[0].length;

        for(int i=0;i<n;i++){

            for(int k=0;k<n-i;k++){
                
                for(int j=0;j<m;j++){
                    System.out.print(input[i][j]+" ");
                }
                System.out.println();
                
            }
            System.out.println();
        }

    }
}
