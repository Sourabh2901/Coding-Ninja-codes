public class Solution {
    public static void deleteAlternateNodes(LinkedListNode<Integer> head) {   
        LinkedListNode<Integer> t1 = head;       
        while(t1.next != null){
            t1.next = t1.next.next;
            t1 = t1.next;
            if(t1 == null){
                break;
            }
        }
        if(t1 != null){
            t1 = t1.next;
        }
    }
}
