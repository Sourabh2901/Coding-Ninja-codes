	public static int indexOfNRec(LinkedListNode<Integer> head, int n) {
		if(head == null){
            return -1;
        }      
        if(head.data == n){
            return 0;
        }
        int ans = indexOfNRec(head.next ,n);
        if(ans >= 0){
            return ans+1;
        }else{
            return ans;
        }
	}
}
